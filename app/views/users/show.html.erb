<h2> <%= @user.first_name %>'s Dashboard</h2>
  <div class="container-fluid debug">
    <div class="row">
      <div class="col-md-3">
        <div class="col-sm-12 col-md-6 sidebar">
          <ul class="nav nav-sidebar debug">
            <li class="active"><%= link_to "Dashboard", user_path(@user) %><span class="sr-only">(current)</span></a></li>
