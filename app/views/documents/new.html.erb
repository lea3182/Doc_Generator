<%= form_for :document, url:  user_documents_path, html: {multipart: true} do |f| %>
<%= f.label :title %>
<%= f.text_field :title %></br>
<%= f.label :amount %>
<%= f.text_field :amount %></br>
<%= f.label :down_payment %>
<%= f.text_field :down_payment %></br>
<%= f.label :interest_rate %>
<%= f.text_field :interest_rate %></br>
<%= f.label :doc_pdf %>
<%= f.file_field :doc_pdf %>
<%= f.submit %>
<% end %>